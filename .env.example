# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key
# Error Tracking
NEXT_PUBLIC_SENTRY_DSN=your_sentry_dsn
SENTRY_AUTH_TOKEN=your_sentry_auth_token
SENTRY_PROJECT=your_sentry_project
SENTRY_ORG=your_sentry_org

# Analytics
NEXT_PUBLIC_VERCEL_ANALYTICS_ID=your_vercel_analytics_id

# Email Service (Resend.com)
RESEND_API_KEY=your_resend_api_key
SMTP_FROM=your_smtp_from
SMTP_HOST=your_smtp_host
SMTP_PORT=587
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password

# Rate Limiting & Caching
UPSTASH_REDIS_URL=your_upstash_redis_url
UPSTASH_REDIS_TOKEN=your_upstash_redis_token
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000
CACHE_TTL=3600

# Feature Flags
NEXT_PUBLIC_ENABLE_BETA_FEATURES=false
NEXT_PUBLIC_MAINTENANCE_MODE=false

# Security
NEXT_PUBLIC_API_URL=your_api_url
NEXT_PUBLIC_APP_URL=your_app_url
JWT_SECRET=your_jwt_secret
COOKIE_SECRET=your_cookie_secret

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Stripe Products
STRIPE_STARTER_PRODUCT_ID=your_starter_product_id
STRIPE_PRO_PRODUCT_ID=your_pro_product_id
STRIPE_BUSINESS_PRODUCT_ID=your_business_product_id

# Stripe Price IDs (Monthly)
STRIPE_STARTER_PRICE_MONTHLY=your_starter_price_monthly
STRIPE_PRO_PRICE_MONTHLY=your_pro_price_monthly
STRIPE_BUSINESS_PRICE_MONTHLY=your_business_price_monthly

# Stripe Price IDs (Yearly)
STRIPE_STARTER_PRICE_YEARLY=your_starter_price_yearly
STRIPE_PRO_PRICE_YEARLY=your_pro_price_yearly
STRIPE_BUSINESS_PRICE_YEARLY=your_business_price_yearly

# Development
NODE_ENV=development
DEBUG=app:*

# API Keys
APIFY_API_TOKEN=your_apify_api_token
OPENAI_API_KEY=your_openai_api_key

# Site Configuration
NEXT_PUBLIC_SITE_URL=your_site_url
